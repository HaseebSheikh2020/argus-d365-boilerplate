{
  "name": "d365-webressources",
  "version": "1.0.0",
  "description": "Project for build webresources with typescript and webpack",
  "author": "Avantic AG",
  "scripts": {
    "dev": "webpack -c ./config/webpack.config.js --mode development",
    "watch": "webpack -c ./config/webpack.config.js --mode development --watch",
    "deploy": "webpack --config ./config/webpack.config.js --mode development --env publish",
    "devserver": "webpack serve --open --config ./config/webpack.config.js --mode development",
    "build": "webpack --config ./config/webpack.config.js --mode production",
    "prettier": "prettier --write src",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^5.1.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "script-loader": "^0.7.2"
  }
}
